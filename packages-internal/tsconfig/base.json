{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noEmit": true,
    "composite": true,
    "strict": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  }
}
